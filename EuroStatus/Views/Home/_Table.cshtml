<div class='liveExample'>
    <table class="ko-grid" cellspacing="3">
        <thead>
            <tr>
                <th>Position</th>
                <th>Name</th>
                <th>Score</th>
            </tr>
        </thead>
              <tbody data-bind="foreach     : players">
                  <tr>
                      <td data-bind="text: index"></td>
                      <td>
                          @*<a data-bind='attr: { href: "/?view=" + name() }, text: name'></a>*@
                          <a data-bind=' click: $root.showPlayer, text: name '></a>
                      </td>
                      <td data-bind="text: score"></td>
                            </tr>
                        </tbody>
    </table>
     
    
</div>